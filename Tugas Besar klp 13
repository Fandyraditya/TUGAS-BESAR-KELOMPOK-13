#include <time.h>
#include <stdio.h>
#include <stdlib.h>
char plat[10];#include <time.h>
#include <stdio.h>
#include <stdlib.h>
char plat[10];
char userName[10];
char password[8];
char ulang;
int jenis;
float biaya;
float masuk;
int totalJam;
float keluar;
float total_biaya;
float tambahan=1000;
int main(){
    time_t currentTime;
    time (&currentTime);
    login:
    printf("=======PLEASE LOGIN=========\n");
    printf("USERNAME = ");
    scanf("%s",&userName);
    printf("PASSWORD = ");
    scanf("%s",&password);
    if(strcmp(userName,"admin")==0 && strcmp(password,"123")==0){
        printf("Akses diterima. Selamat Datang\n");
        system("pause");}
    else{
        printf("LOGIN GAGAL COBA LAGI\n");
        system ("pause");
        system ("cls");
        goto login;
    }


    mulai:
printf("=======================================\n");
printf("||            TIKET PARKIR           ||\n");
printf("=======================================\n");
printf("%s\n", ctime(&currentTime));
printf(" Plat Kendaraan       = ");
scanf("%s",&plat);
printf(" kode jenis kendaraan = ");
scanf("%d",&jenis);
if(jenis==2)
{
    biaya=2000;
printf(" kendaraan            = motor\n");
printf(" biaya parkir         = Rp%.2f\n",biaya);
}
else if(jenis==4)
{
    biaya=5000;
printf(" kendaraan            = mobil\n");
printf(" biaya parkir         = Rp.%.2f\n",biaya);}
else {
    printf("UNKNOWN CODE\n");
    system("pause");
    system("cls");
    goto mulai;
}

printf(" jam masuk            = ");
scanf("%f",&masuk);

printf(" jam keluar           = ");
scanf("%f",&keluar);

totalJam = keluar-masuk;
printf(" total jam parkir     = %d jam\n",totalJam);

total_biaya = totalJam*tambahan+biaya;
printf(" total biaya parkir   = Rp.%.2f",total_biaya);
printf("\n==============TERIMA KASIH=============\n");
printf("==========SILAHKAN DATANG LAGI=========\n\n");


pengulangan:
printf("Transaksi lagi? (Y/N) = ");
scanf("%c",&ulang);
if(ulang=='Y'||ulang=='y'){
    printf("memulai ulang transaksi\n");
    system("pause");
    system("cls");
    goto mulai;
}
else if (ulang=='N'||ulang=='n'){
    printf("kembali ke menu login\n");
    system ("pause");
    system ("cls");
    goto login;
}
else{
    printf("\nERROR CODE\n");
    goto pengulangan;
}
return 0;
    }



