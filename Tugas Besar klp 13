#include <stdio.h>
#include <stdlib.h>
char plat[10];
int jenis=2;
float biaya;
float masuk;
int total;
float keluar;
float total_biaya;
float tambahan=1000;
int main(){

printf("============Tugas Besar Pemrograman==========\n");
printf("||    kelompok 13                          ||\n");
printf("||    Anggota Kelompok :                   ||\n");
printf("||    Akbar Bimadi                         ||\n");
printf("||    Gede Raditya Arya Wiguna             ||\n");
printf("||    Fandy Kusumaraditya Datar            ||\n");
printf("=============================================\n");

//input jenis kendaraan yang parkir
printf("\n\nmasukkan Plat Kendaraan = ");
scanf("%s",&plat);

printf("jenis kendaraan = ");


scanf("%d",&jenis);
if(jenis==2)
{
    biaya=2000;
    printf("kendaraan = motor\n");
    printf("biaya parkir = Rp%.2f",biaya);
}
else if(jenis==4)
{
    biaya=5000;
    printf("kendaraan = mobil\n");
    printf("biaya parkir = Rp.%.2f\n",biaya);
}
printf("%d",jenis);

printf("jam masuk =");
scanf("%f,masuk);

printf("jam keluar =");
scanf("%f,keluar);

total = keluar-masuk;

printf("total jam parkir = %.2f,total");

total_biaya = jenis+(total*tambahan);
printf("total biaya parkir = Rp.%.2f,total_biaya");


 return 0;
}
